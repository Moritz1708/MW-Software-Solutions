
<MudCard Outlined="true" Elevation="5">
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Class="fw-bold" Align="Align.Center" Typo="Typo.body2">
                @Title
            </MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent Style="height: 200px;">
        
    </MudCardContent>
</MudCard>

@code {
    [Parameter]
    public string? Title { get; set; }
    [Parameter]
    public string? SourcePath { get; set; }
}